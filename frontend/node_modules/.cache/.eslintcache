;\nimport {useSelector, useDispatch} from \"react-redux\";\n\nfunction ProductScreen(props) {\n    const productId = props.match.params.id; //props.match.params.id: user entered in Route path=\"/product/:id\"\n    const dispatch = useDispatch();\n    const productDetails = useSelector(state => state.productDetails);\n    const {loading, product, error} = productDetails;\n    useEffect(() => { \n        dispatch(detailsProduct(productId))\n    }, [dispatch, productId]);\n    return (\n        <div>\n        {\n            loading ? (\n            <LoadingBox />\n            ) : error ? (\n            <MessageBox variant=\"danger\">{error}</MessageBox>\n            ) : (\n                <div>\n                <Link to=\"/\">Back</Link>\n                <div className=\"row top\">\n                    <div className=\"col-2\">\n                        <img className=\"large\" src={product.image} alt={product.name}/>\n                    </div>\n                    <div className=\"col-1\">\n                        <ul>\n                            <li><h1>{product.name}</h1></li>\n                            <li><Ratings rating={product.rating} numReviews={product.numReviews} /></li>\n                            <li>Price: {product.price}</li>\n                            <li>Description: <p>{product.description}</p></li>\n                        </ul>\n                    </div>\n                    <div className=\"col-1\">\n                        <div className=\"card card-body\">\n                            <ul>\n                                <li>\n                                    <div className=\"row\">\n                                        <div>Price</div>\n                                        <div className=\"price\">${product.price}</div>\n                                    </div>\n                                </li>\n                                <li>\n                                    <div className=\"row\">\n                                        <div>Status</div>\n                                        <div>\n                                        {product.countInStock > 0 ? (\n                                            <span className=\"success\">In Stock</span>\n                                        ) : (\n                                            <span className=\"danger\">Unavailable</span>\n                                        )}\n                                        </div>\n                                    </div>\n                                </li>\n                                <li>\n                                    <button className=\"primary block\">Add To Cart</button>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            )\n        }\n        </div>\n        \n    )\n}\n\nexport default ProductScreen;\n\n/* replace to redux store\nconst product = data.products.find(item => item._id === props.match.params.id); \nconst isMatch = item => item._id === props.match.params.id\nconst product = data.products[data.products.findIndex(isMatch)]\nif(!product){ \n    return <div> Product Not Found! Please Search for Another Product </div>\n}\n*/","/Users/chenlicheng/Desktop/Ecommerce/frontend/src/components/Products.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/components/Ratings.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/components/LoadingBox.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/components/MessageBox.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/store.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/reducers/productReducers.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/actions/productActions.js",[],"/Users/chenlicheng/Desktop/Ecommerce/frontend/src/constants/productConstants.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"6